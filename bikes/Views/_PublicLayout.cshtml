    <nav class="navbar navbar-inverse navbar-static-top">
        <div class="container">
            <div id="navbar" class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>
                        <a href="#">Drurys Bike Rides - Login</a>
                    </li>
                </ul>
            </div><!--/.nav-collapse -->
